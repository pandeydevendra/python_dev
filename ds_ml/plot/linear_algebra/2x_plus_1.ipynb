    "import matplotlib.pyplot as plt\n",
    "\n",
    "\n",
    "def f(x):\n",
    "    y_at_x = 2 * x + 1\n",
    "    return y_at_x\n",
    "\n",
    "\n",
    "x = [number for number in range(-10, 11)]\n",
    "print(x)\n",
    "y = [f(x_p) for x_p in x]\n",
    "print(y)\n",
    "\n",
    "plt.plot(x, y)\n",
    "plt.xlabel(\"x--->\")\n",
    "plt.ylabel(\"y--->\")\n",
    "plt.title(\"2x + 1\")\n",
    "plt.show()"
   ]
